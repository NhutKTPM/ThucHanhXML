{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Weather Data",
  "type": "object",
  "properties": {
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "temperature": {
      "type": "number"
    },
    "humidity": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "wind_speed": {
      "type": "number"
    },
    "description": {
      "type": "string"
    }
  },
  "required": [
    "city",
    "country",
    "temperature",
    "humidity",
    "wind_speed",
    "description"
  ]
}